{
  "name": "nuxt-sanity-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "echo 'Building Sanity to static/studio...' && npm install -g @sanity/cli && cd studio && npx sanity build ../static/studio -y && cd .. && nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "postinstall": "cd studio && npm install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/sanity": "^0.10.0",
    "@sanity/client": "^2.21.10",
    "@sanity/image-url": "^1.0.1",
    "core-js": "^3.19.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/image": "^0.6.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@sanity/cli": "^2.21.10",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fibers": "^5.0.0",
    "jest": "^27.0.5",
    "sass": "^1.44.0",
    "sass-loader": "^10.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^4.0.0",
    "vue-jest": "^3.0.4"
  }
}
